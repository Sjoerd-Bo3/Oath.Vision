{
  "name": "oath-vision",
  "description": "A viewer and lineage tracker for Oath board games.",
  "version": "0.0.1",
  "author": "Kyle Kemp <kyle@seiyria.com>",
  "homepage": "https://oath.vision/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --port 6984 --host=0.0.0.0",
    "build": "npm run asset:pack && ng build --prod",
    "build:withresources": "ng build --prod",
    "test": "npm run lint && npm run validate && npm run build",
    "lint": "ng lint",
    "asset:pack": "npm run asset:pack:edifice && npm run asset:pack:relic && npm run asset:pack:site && npm run asset:pack:world && npm run asset:crush",
    "asset:pack:edifice": "node scripts/pack-edifices",
    "asset:pack:relic": "node scripts/pack-relics",
    "asset:pack:site": "node scripts/pack-sites",
    "asset:pack:world": "node scripts/pack-world",
    "asset:crush": "node scripts/pngquant",
    "postinstall": "npm run asset:pack",
    "validate": "npm run validate:images",
    "validate:images": "node test/validate-images-exist"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~10.0.0",
    "@angular/core": "~10.0.0",
    "@angular/fire": "^6.0.2",
    "@angular/forms": "~10.0.0",
    "@angular/platform-browser": "~10.0.0",
    "@angular/platform-browser-dynamic": "~10.0.0",
    "@angular/router": "~10.0.0",
    "@ionic-native/core": "^5.0.0",
    "@ionic-native/splash-screen": "^5.0.0",
    "@ionic-native/status-bar": "^5.0.0",
    "@ionic/angular": "^5.5.1",
    "@seiyria/oath-parser": "^1.0.7",
    "censor-sensor": "^1.0.6",
    "clipboard": "^2.0.6",
    "firebase": "^7.18.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": ">= 0.900 < 0.1100",
    "@angular-devkit/build-angular": "~0.1000.0",
    "@angular/cli": "~10.0.5",
    "@angular/compiler": "~10.0.0",
    "@angular/compiler-cli": "~10.0.0",
    "@angular/language-service": "~10.0.0",
    "@ionic/angular-toolkit": "^2.3.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "firebase-tools": "^8.0.0",
    "fuzzy": "^0.1.3",
    "imagemin": "^7.0.1",
    "imagemin-pngquant": "^9.0.0",
    "imagemin-webp": "^6.0.0",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "open": "^7.0.3",
    "rimraf": "^3.0.2",
    "spritesheet-js": "^1.2.6",
    "ts-node": "^8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5"
  }
}
